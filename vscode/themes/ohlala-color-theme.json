  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Diff",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#68737DFF"
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#51FBDEFF"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#FF6B9DFF"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#9B6DFFFF"
      }
    },
    {
      "name": "Error",
      "scope": [
        "markup.error"
      ],
      "settings": {
        "fontStyle": "underline italic",
        "foreground": "#FF6B9DFF"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "fontStyle": "underline italic",
        "foreground": "#FF6B9DFF"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "fontStyle": "underline italic",
        "foreground": "#FF6B9DFF"
      }
    },
    {
      "name": "Entity name filename",
      "scope": [
        "entity.name.filename"
      ],
      "settings": {
        "foreground": "#9A89B4FF"
      }
    },
    {
      "name": "Text",
      "scope": [
        "meta.paragraph.markdown",
        "text.html.markdown"
      ],
      "settings": {
        "foreground": "#D9E0E8FF"
      }
    },
    {
      "name": "Underlined markup",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Bold markup",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#00B2FFFF"
      }
    },
    {
      "name": "Markup headings",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "fontStyle": "bold underline",
        "foreground": "#9B6DFFFF"
      }
    },
    {
      "name": "Markup italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FED604FF"
      }
    },
    {
      "name": "Bullets, lists (prose)",
      "scope": [
        "punctuation.definition.list.begin.markdown",
        "beginning.punctuation.definition.list.markdown",
        "beginning.punctuation.definition.quote.markdown",
        "punctuation.definition.link.restructuredtext"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D9E0E8FF"
      }
    },
    {
      "name": "Inline code (prose)",
      "scope": [
        "markup.inline.raw",
        "markup.raw.restructuredtext"
      ],
      "settings": {
        "foreground": "#FF6B9DFF"
      }
    },
    {
      "name": "Code blocks",
      "scope": [
        "fenced_code.block.language.markdown",
        "fenced_code.block.language",
        "markup.raw.inner.restructuredtext",
        "markup.fenced_code.block.markdown punctuation.definition.markdown"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#68737DFF"
      }
    },
    {
      "name": "Links (prose)",
      "scope": [
        "markup.underline.link",
        "markup.underline.link.image"
      ],
      "settings": {
        "foreground": "#68737DFF"
      }
    },
    {
      "name": "Link text, image alt text (prose)",
