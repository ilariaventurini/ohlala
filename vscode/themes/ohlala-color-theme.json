      "settings": {
        "foreground": "#68737DFF"
      }
    },
    {
      "name": "User-defined class names",
      "scope": [
        "entity.name.type.class"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#D9E0E8FF"
